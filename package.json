{
  "name": "NBT",
  "version": "1.0.0",
  "description": "NBT",
  "engines": {
    "node": ">=7.2.0"
  },
  "scripts": {
    "start": "NODE_ENV=development SITE=nbt npm run serve",
    "startwin": "set NODE_ENV=development&& set SITE=nbt&& npm run serve",
    "serve": "node ./server",
    "build": "node ./tasks/build",
    "prod:build": "SITE=nbt ASSET_PATH=https://static.nbt.indiatimes.com/ npm run build && NODE_ENV=production",
    "prod:serve": "WEBPACK_DEV_SERVER=false NODE_ENV=production SITE=nbt ASSET_PATH=http://localhost:3001/ PORT=3001 npm run serve",
    "prettier": "prettier --write src/**/*.js",
    "lint": "eslint --debug src/",
    "lint:write": "eslint --debug src/ --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js|jsx)": [
      "npm run lint:write",
      "git add"
    ]
  },
  "author": "Sandeep Panwar",
  "license": "MIT",
  "homepage": "https://www.navbharattimes.com",
